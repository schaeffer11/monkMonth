{
  "name": "speedwise-platform-app-web",
  "version": "1.0.0",
  "description": "A new beginning to the Speedwise Platform!",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint",
    "start:server": "babel-node ./server/index.js",
    "watch:server": "nodemon ./server/index.js --exec babel-node --watch server",
    "watch:server-heavy-headers": "nodemon --max-http-header-size=20000 ./server/index.js --exec babel-node --watch server",
    "watch:client": "webpack --progress --watch",
    "watch:client-heavy": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --progress --watch",
    "build": "npm run build:server & npm run build:client",
    "build:server": "rollup -c --environment NODE_ENV:production",
    "build:client": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --progress --verbose",
    "account": "babel-node ./server/auth/users.js"
  },
  "engines": {
    "node": "10.17.x",
    "npm": "6.11.x"
  },
  "author": "Peter & Schaeffer",
